<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_tint">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/LINKTXTTTTTT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="DESC : "
                android:textColor="@color/txt2"
                android:textSize="@dimen/text3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/report_link" />

            <TextView
                android:id="@+id/report_nametxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/p_padding5"
                android:layout_marginTop="44dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding6"
                android:text="Name : "
                android:textAlignment="center"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/report_updatebtn" />

            <Button
                android:id="@+id/report_updatebtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/alata"
                android:text="UPDATE NOW"
                android:textSize="@dimen/text2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView4" />

            <Button
                android:id="@+id/report_delbtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/alata"
                android:text="DELETE"
                android:textSize="@dimen/text2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/report_updatebtn"
                app:layout_constraintTop_toBottomOf="@+id/textView4" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Latest Price : "
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/report_nametxt" />

            <TextView
                android:id="@+id/report_latestPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="TextView"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toEndOf="@+id/textView5"
                app:layout_constraintTop_toTopOf="@+id/textView5" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Frequency of Price change : "
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <TextView
                android:id="@+id/report_freq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="22dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="freq"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toEndOf="@+id/textView7"
                app:layout_constraintTop_toBottomOf="@+id/report_latestPrice" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="22dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Lowest Price : "
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <TextView
                android:id="@+id/report_lowP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="22dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="low"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toEndOf="@+id/textView9"
                app:layout_constraintTop_toBottomOf="@+id/report_freq" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="22dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Heighest Price : "
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:id="@+id/report_heighP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="22dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="high"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toEndOf="@+id/textView11"
                app:layout_constraintTop_toBottomOf="@+id/report_lowP" />

            <!--            <TextView-->
            <!--                android:id="@+id/le_big_sizer"-->
            <!--                android:layout_width="82dp"-->
            <!--                android:layout_height="287dp"-->
            <!--                android:layout_marginStart="130dp"-->
            <!--                android:layout_marginTop="3000dp"-->
            <!--                android:layout_marginBottom="16dp"-->
            <!--                android:text="TextView"-->
            <!--                app:layout_constraintBottom_toBottomOf="parent"-->
            <!--                app:layout_constraintStart_toStartOf="parent"-->
            <!--                app:layout_constraintTop_toTopOf="parent" />-->

            <TextView
                android:id="@+id/report_desc"
                android:layout_width="match_parent"
                android:layout_height="95dp"
                android:layout_margin="@dimen/p_padding6"
                android:layout_marginTop="2dp"
                android:background="@drawable/shapes"
                android:fontFamily="@font/alata"
                android:linksClickable="true"
                android:nestedScrollingEnabled="true"
                android:padding="@dimen/p_padding4"
                android:text="DESC"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/LINKTXTTTTTT" />

            <TextView
                android:id="@+id/report_link"
                android:layout_width="match_parent"
                android:layout_height="108dp"
                android:layout_margin="@dimen/p_padding6"
                android:layout_marginStart="20dp"
                android:background="@drawable/shapes"
                android:ellipsize="start"
                android:fadeScrollbars="true"
                android:fontFamily="@font/alata"
                android:isScrollContainer="false"
                android:lineSpacingExtra="@dimen/p_padding2"
                android:lineSpacingMultiplier="1.43"
                android:lines="5"
                android:linksClickable="true"
                android:nestedScrollingEnabled="false"
                android:overScrollMode="never"
                android:padding="@dimen/p_padding4"
                android:paddingTop="10dp"
                android:paddingEnd="10dp"
                android:paddingBottom="10dp"
                android:scrollbars="none"
                android:scrollHorizontally="false"
                android:singleLine="false"
                android:text="LINK cfcghcvghjftvfgvgfgtvgftvfghvgfgvgftfvtfvtftftftftftftftfttf fdgvsgjdsfgisdfgjdisfjgisgjisfdgjidsfgjnifdngjifgbfdibgjidfgbjifogjifgjdifbjgidfosgjifpgjdifgjdifbgjdisfgjiod"
                android:textAlignment="textStart"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.51"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/TextView10"
                app:lineHeight="@dimen/text5" />

            <Button
                android:id="@+id/report_editbtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/alata"
                android:text="EDIT"
                app:layout_constraintEnd_toStartOf="@+id/report_updatebtn"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView4" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="25dp"
                android:layout_marginTop="22dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Refresh rate : "
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/report_desc" />

            <TextView
                android:id="@+id/report_refresh"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="ref"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toEndOf="@+id/textView16"
                app:layout_constraintTop_toTopOf="@+id/textView16" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/url_final_notifications_constraint_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/p_padding6"
                android:layout_marginTop="22dp"
                android:background="@drawable/shapes"
                android:padding="@dimen/p_padding4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16">

                <TextView
                    android:id="@+id/report_notifs_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="11dp"
                    android:fontFamily="@font/alata"
                    android:padding="@dimen/p_padding3"
                    android:text="Notifications "
                    android:textColor="@color/txt2"
                    android:textSize="@dimen/text4"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/report_notifs_increase_TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/alata"
                    android:padding="@dimen/p_padding3"
                    android:text="Increase in Price : "
                    android:textColor="@color/txt1"
                    android:textSize="@dimen/text3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/report_notifs_textView" />

                <TextView
                    android:id="@+id/report_notifs_decrease_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="12dp"
                    android:layout_marginBottom="10dp"
                    android:fontFamily="@font/alata"
                    android:padding="@dimen/p_padding3"
                    android:text="Decrease in Price :"
                    android:textColor="@color/txt1"
                    android:textSize="@dimen/text3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/report_notifs_increase_TextView" />

                <TextView
                    android:id="@+id/report_notifinc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:fontFamily="@font/alata"
                    android:padding="@dimen/p_padding3"
                    android:text="TextView"
                    android:textColor="@color/green"
                    android:textSize="@dimen/text3"
                    app:layout_constraintStart_toEndOf="@+id/report_notifs_increase_TextView"
                    app:layout_constraintTop_toTopOf="@+id/report_notifs_increase_TextView" />

                <TextView
                    android:id="@+id/report_notifdec"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:fontFamily="@font/alata"
                    android:padding="@dimen/p_padding3"
                    android:text="TextView"
                    android:textColor="@color/red"
                    android:textSize="@dimen/text3"
                    app:layout_constraintStart_toEndOf="@+id/report_notifs_decrease_textView"
                    app:layout_constraintTop_toTopOf="@+id/report_notifs_decrease_textView" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <TableLayout
                android:id="@+id/report_table_layout"
                android:layout_width="377dp"
                android:layout_height="wrap_content"
                android:layout_margin="30dp"
                android:layout_marginTop="22dp"
                android:background="@drawable/shapes"
                android:padding="@dimen/p_padding4"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/url_final_notifications_constraint_layout" />

            <TextView
                android:id="@+id/TextView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="36dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="LINK : "
                android:textColor="@color/txt2"
                android:textSize="@dimen/text3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.015"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView11" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/l_ad_height"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>