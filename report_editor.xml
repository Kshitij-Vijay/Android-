<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_tint">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/LINKTXTTTTTT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="33dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="DESC : "
                android:textColor="@color/txt2"
                android:textSize="@dimen/text3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.059"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/report_editor_link" />

            <TextView
                android:id="@+id/DESCTTTTXTTTT"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="22dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="LINK : "
                android:textColor="@color/txt2"
                android:textSize="@dimen/text3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.058"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView11" />

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/report_editor_nametxt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/p_padding5"
                android:layout_marginTop="60dp"
                android:fontFamily="@font/alata"
                android:text="Name : "
                android:textAlignment="center"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:textInputLayoutFocusedRectEnabled="false" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Latest Price : "
                android:textColor="@color/txt3"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/report_editor_nametxt" />

            <TextView
                android:id="@+id/report_editor_latestPrice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="TextView"
                android:textColor="@color/txt3"
                android:textSize="@dimen/text3"
                app:layout_constraintBottom_toBottomOf="@+id/textView5"
                app:layout_constraintStart_toEndOf="@+id/textView5"
                app:layout_constraintTop_toTopOf="@+id/textView5" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Frequency of Price change : "
                android:textColor="@color/txt3"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView5" />

            <TextView
                android:id="@+id/report_editor_freq"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="26dp"
                android:fontFamily="@font/alata"
                android:minWidth="48dp"
                android:padding="@dimen/p_padding3"
                android:text="freq"
                android:textColor="@color/txt3"
                android:textSize="@dimen/text3"
                app:layout_constraintBottom_toBottomOf="@+id/textView7"
                app:layout_constraintStart_toEndOf="@+id/textView7"
                app:layout_constraintTop_toTopOf="@+id/textView7" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Lowest : "
                android:textColor="@color/txt3"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <TextView
                android:id="@+id/report_editor_lowP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="26dp"
                android:fontFamily="@font/alata"
                android:minWidth="48dp"
                android:text="low"
                android:textColor="@color/txt3"
                android:textSize="@dimen/text3"
                app:layout_constraintBottom_toBottomOf="@+id/textView9"
                app:layout_constraintStart_toEndOf="@+id/textView9"
                app:layout_constraintTop_toTopOf="@+id/textView9" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Heighest"
                android:textColor="@color/txt3"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView9" />

            <TextView
                android:id="@+id/report_editor_heighP"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="26dp"
                android:fontFamily="@font/alata"
                android:minWidth="48dp"
                android:padding="@dimen/p_padding3"
                android:text="high"
                android:textColor="@color/txt3"
                android:textSize="@dimen/text3"
                app:layout_constraintBottom_toBottomOf="@+id/textView11"
                app:layout_constraintStart_toEndOf="@+id/textView11"
                app:layout_constraintTop_toTopOf="@+id/textView11" />

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/report_editor_desc"
                android:layout_width="358dp"
                android:layout_height="95dp"
                android:layout_margin="@dimen/p_padding6"
                android:layout_marginTop="2dp"
                android:background="@drawable/ripple_effect"
                android:fontFamily="@font/alata"
                android:linksClickable="true"
                android:nestedScrollingEnabled="true"
                android:padding="@dimen/p_padding4"
                android:paddingTop="10dp"
                android:paddingEnd="10dp"
                android:paddingBottom="5dp"
                android:text="DESC"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/LINKTXTTTTTT" />

            <TextView
                android:id="@+id/report_editor_link"
                android:layout_width="364dp"
                android:layout_height="48dp"
                android:layout_margin="@dimen/p_padding6"
                android:layout_marginTop="3dp"
                android:background="@drawable/ripple_effect"
                android:fontFamily="@font/alata"
                android:linksClickable="true"
                android:padding="@dimen/p_padding4"
                android:paddingTop="10dp"
                android:paddingEnd="10dp"
                android:paddingBottom="10dp"
                android:text="LINK"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.51"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/DESCTTTTXTTTT" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="20dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding3"
                android:text="Refresh : "
                android:textColor="@color/txt3"
                android:textSize="@dimen/text3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/report_editor_desc" />

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/report_editor_refresh"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="26dp"
                android:fontFamily="@font/alata"
                android:inputType="number|numberDecimal"
                android:minWidth="48dp"
                android:padding="@dimen/p_padding3"
                android:text="ref"
                android:textAlignment="center"
                android:textColor="@color/txt1"
                android:textSize="@dimen/text3"
                app:layout_constraintBottom_toBottomOf="@+id/textView16"
                app:layout_constraintStart_toEndOf="@+id/textView16"
                app:layout_constraintTop_toTopOf="@+id/textView16" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/url_final_notifications_constraint_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/p_padding6"
                android:layout_marginTop="22dp"
                android:layout_marginEnd="30dp"
                android:background="@drawable/shapes"
                android:padding="@dimen/p_padding3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView16">

                <TextView
                    android:id="@+id/report_editor_notifs_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="24dp"
                    android:fontFamily="@font/alata"
                    android:padding="@dimen/p_padding3"
                    android:text="Notifications : "
                    android:textColor="@color/txt3"
                    android:textSize="@dimen/text3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/report_editor_notifs_increase_TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="25dp"
                    android:fontFamily="@font/alata"
                    android:padding="@dimen/p_padding3"
                    android:text="Increase in Price : "
                    android:textColor="@color/txt3"
                    android:textSize="@dimen/text3"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/report_editor_notifs_textView" />

                <TextView
                    android:id="@+id/report_editor_notifs_decrease_textView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginBottom="10dp"
                    android:fontFamily="@font/alata"
                    android:padding="@dimen/p_padding3"
                    android:text="Decrease in Price :"
                    android:textColor="@color/txt3"
                    android:textSize="@dimen/text3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/report_editor_notifs_increase_TextView" />

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/report_editor_notifinc"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="26dp"
                    android:background="@drawable/ripple_effect"
                    android:fontFamily="@font/alata"
                    android:inputType="number|numberDecimal"
                    android:padding="@dimen/p_padding3"
                    android:text="TextView"
                    android:textColor="@color/green"
                    android:textSize="@dimen/text3"
                    app:layout_constraintBottom_toBottomOf="@+id/report_editor_notifs_increase_TextView"
                    app:layout_constraintStart_toEndOf="@+id/report_editor_notifs_increase_TextView"
                    app:layout_constraintTop_toTopOf="@+id/report_editor_notifs_increase_TextView" />

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/report_editor_notifdec"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="26dp"
                    android:background="@drawable/ripple_effect"
                    android:fontFamily="@font/alata"
                    android:inputType="number|numberDecimal"
                    android:padding="@dimen/p_padding3"
                    android:text="TextView"
                    android:textColor="@color/red"
                    android:textSize="@dimen/text3"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/report_editor_notifs_decrease_textView"
                    app:layout_constraintTop_toTopOf="@+id/report_editor_notifs_decrease_textView"
                    app:textInputLayoutFocusedRectEnabled="false" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <Button
                android:id="@+id/report_editor_done"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/p_padding4"
                android:layout_marginTop="50dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding5"
                android:text="Done"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/url_final_notifications_constraint_layout" />

            <Button
                android:id="@+id/report_editor_cancel"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/p_padding4"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:fontFamily="@font/alata"
                android:padding="@dimen/p_padding5"
                android:text="Cancel"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/report_editor_done" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>